{
  "manifest_version": 2,
  "name": "Facebook without Profile Pictures and Names",
  "version": "0.1",
  "content_scripts": [{
    "matches": [
      "*://www.facebook.com/*"
    ],
    "js": ["jquery-2.2.4.min.js", "content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
